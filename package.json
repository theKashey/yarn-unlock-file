{
  "name": "yarn-unlock-file",
  "version": "1.0.0",
  "main": "index.js",
  "description": "Unlock versions of your indirect dependencies",
  "author": "Anton Korzunov <thekashey@gmail.com>",
  "license": "MIT",
  "bin": {
    "yarn-unlock-file": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "yarn",
    "yarn.lock",
    "indirect",
    "dependencies"
  ],
  "scripts": {
    "prepublishOnly": "yarn build",
    "compile": "tsc",
    "set-permissions": "chmod +x ./dist/cli.js",
    "build": "yarn compile && yarn set-permissions",
    "test": "node ./dist/cli.js levels all"
  },
  "dependencies": {
    "@manypkg/find-root": "^1.1.0",
    "@manypkg/get-packages": "^1.1.3",
    "@yarnpkg/lockfile": "^1.1.0",
    "memoize-one": "^6.0.0",
    "sade": "^1.8.1"
  },
  "devDependencies": {
    "@types/yarnpkg__lockfile": "^1.1.5",
    "typescript": "^4.7.4"
  }
}
